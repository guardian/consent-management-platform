{
  "name": "cdk",
  "version": "0.0.0",
  "private": true,
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:dev": "jest --watch",
    "format": "prettier --write \"{lib,bin}/**/*.ts\"",
    "cdk": "cdk",
    "lint": "eslint lib/** bin/** --ext .ts --no-error-on-unmatched-pattern",
    "synth": "cdk synth --path-metadata false --version-reporting false",
    "diff": "cdk diff --path-metadata false --version-reporting false"
  },
  "devDependencies": {
    "@guardian/eslint-config-typescript": "^5.0.0",
    "@types/jest": "^27.0.2",
    "@types/node": "16.11.7",
    "aws-cdk": "2.45.0",
    "aws-cdk-lib": "2.45.0",
    "constructs": "10.1.127",
    "eslint": "^7.32.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "~4.4.3"
  },
	"resolutions":{
		"//": "Once Jest upgrades its transitive dependency on json5 >=2.0.0, <2.2.2, remove this resolution",
		"json5": "^2.2.2"
	},
  "dependencies": {
    "@guardian/cdk": "48.0.1",
    "npm": "^8.15.0",
    "source-map-support": "^0.5.20"
  }
}
